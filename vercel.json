{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/**",
      "use": "@vercel/node"
    },
    {
      "src": "models/**",
      "use": "@vercel/node"
    },
    {
      "src": "controllers/**",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
